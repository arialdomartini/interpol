def renderItem(item)
  <li class=item.type id="id-%" % item.id selected>
    item.name
  </li>
end

def renderList(list)
  <ul>
  for item in list
    renderItem(item)
  end
  </ul>
end

<html>
  <head>
    <title>"a static title"</title>
    <!-- this is a comment -->
  </head>
  <body>
    "this is a list with % items" % list.length
    renderList(list)
  </body>
</html>
