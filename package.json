{
  "name": "interpol",
  "description": "Templates Sans Facial Hair",
  "author": "Thomas S. Bradford",
  "version": "0.1.6",
  "license": "MIT",
  "homepage": "http://www.interpoljs.io/",
  "main": "index.js",
  "bin": {
    "interpol": "lib/cli.js"
  },
  "keywords": ["javascript", "html", "nodejs", "browser", "templates"],
  "contributors": [
    { "name": "Thom Bradford", "email": "thom@kode4food.it" }
  ],
  "repository": "git://github.com/kode4food/interpol",
  "scripts": {
    "build": "pegjs lib/parser.pegjs build/parser.js && browserify browserify.js > build/interpol.js && uglifyjs -m --preamble \"`cat doc/preamble.js`\" build/interpol.js -o build/interpol.min.js && browserify browserify-parser.js | uglifyjs -m --preamble \"`cat doc/preamble.js`\" -o build/interpol-parser.min.js",
    "test": "nodeunit test/index.js"
  },
  "dependencies": {
    "mkdirp": "*",
    "glob": "*"
  },
  "devDependencies": {
    "pegjs": "*",
    "nodeunit": "*",
    "uglify-js": "*",
    "browserify": "*"
  },
  "engines": {
    "node": "*"
  }
}
